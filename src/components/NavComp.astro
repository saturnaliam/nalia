---
interface Props {
  name: string;
  link: string;
}
const { name, link } = Astro.props;
---

<li>
  <a
    href={"/nalia" + link}
    class="block text-lg py-2 pl-3 pr-4 text-zinc-900 rounded hover:bg-zinc-100 md:hover:bg-transparent md:border-0 md:hover:text-blue-700 md:p-0 dark:text-white md:dark:hover:text-blue-500 dark:hover:bg-zinc-700 dark:hover:text-white md:dark:hover:bg-transparent"
    >{name}</a
  >
</li>
